{
  "name": "@maxbo/free-port",
  "version": "1.0.5",
  "description": "A utility to free a port prior to use. Add this before starting a server and it will kill the process using the port, optionally prompting before doing so. It will also stop docker containers without stopping the entire docker process.",
  "author": "MaxboDev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxboDev/free-port.git"
  },
  "license": "MIT",
  "main": "bin/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "nyc npm test",
    "version:patch": "npm version patch && git push --follow-tags",
    "version:minor": "npm version minor && git push --follow-tags",
    "version:major": "npm version major && git push --follow-tags"
  },
  "keywords": [
    "free",
    "port",
    "kill",
    "process",
    "stop",
    "docker",
    "container"
  ],
  "bin": {
    "free-port": "./bin/index.js"
  },
  "dependencies": {
    "find-process": "^1.4.7",
    "wait-for-expect": "^3.0.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "exclude": [
      "**/tests/**"
    ]
  }
}
